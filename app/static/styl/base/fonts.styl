    url('../font/theinhardt-bold.ttf') format('truetype'),
    url('../font/theinhardt-bold.otf') format('opentype'),
    url('../font/theinhardt-bold.svg#theinhardt-bold') format('svg')
  font-weight 700
  font-style normal
  font-stretch normal
  unicode-range U+0020-FB02
